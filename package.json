{
  "name": "serum-multisig-cli",
  "version": "2.0.0",
  "description": "serum multisig cli",
  "main": "src/cli.ts",
  "scripts": {
    "build": "tsc",
    "format:check": "prettier --list-different 'src/**/*.ts'",
