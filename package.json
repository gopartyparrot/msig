    "commander": "^8.2.0",
    "env-cmd": "^10.1.0",
    "js-base64": "^3.7.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}